{
  "manifest_version": 3,
  "name": "easework",
  "version": "1.0.0",
  "icons": {
    "16": "./up.png",
    "48": "./up.png",
    "128": "./up.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs","storage", "alarms"],
  "host_permissions": ["*://*/*"]
}
